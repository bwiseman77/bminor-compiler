// find max value in array
a:array [10] integer = {4, 7, 17, 14, 35, 13, 34, 75, 76, 21};
size:integer = 10;

main:function integer () = {
    max:integer = 0;
    i:integer = 0;
    for(i=0; i<size; i++) {
        if(a[i] > max) {
            max = a[i];
        }
    }
    print max, "\n";
    return 0;
}
